#include <iostream>
using namespace std;

int main() {
    int arr[]={16,17,4,3,5,2};
    int n=sizeof (arr)/(sizeof(arr[0]));
    
    int leaders[n];
    int c=0;
    
    int max_from_right=arr[n-1];
    leaders[c++]=max_from_right;
    
    for(int I=n-2; I>=0; I--)
    {
        if(arr[I] > max_from_right) 
        {
            leaders [c++]=arr[I];
            max_from_right=arr[I];
        }
    }
    cout<<"leaders: ";
    for(int I=c-1; I>=0;I--){
    cout<<leaders [I]<<" ";
  }
    cout<<endl;

    return 0;
}