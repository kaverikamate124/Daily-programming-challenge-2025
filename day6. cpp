#include <iostream>
using namespace std;

int main() {
    int arr[]={1,2,-3,3,-1,2};
    //int arr[]={17, 29};
    int n=sizeof(arr)/(sizeof(arr[0]));
     
    bool list=false;
    for(int I=0; I<n; I++){
        int sum=0;
        
        for(int j=I;j<n;j++){
            sum=sum+arr[j];
            
            if(sum==0){
                cout<<"("<<I<<","<<j<<") ";
                list=true;
            }
        }
    }
    if(!list){
        cout<<"[]";
    }
    return 0;
}