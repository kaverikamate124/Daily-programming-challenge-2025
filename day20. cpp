#include <iostream>
#include <stack>
using namespace std;

void insert_sortedstack ( stack<int>& st, int element) {
    if (st.empty()||element<st.top()){
        st.push(element);
        return;
    }
    int t=st.top();st.pop();
    insert_sortedstack(st,element);
    st.push(t);
}
void sortStack(stack<int>& st) {
    if (st.empty()) 
    return;
    int t=st.top();st.pop();
    sortStack(st);
    insert_sortedstack(st, t);
}
int main() {
    stack<int>st;
    st.push(3);
    st.push(1);
    st.push(4);
    st.push(2);
    sortStack(st);
    cout << "Output: ";
    while (!st.empty()){
        cout <<st.top() << " ";
        st.pop();
    }
    cout<<endl;
    return 0;
}