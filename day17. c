#include <stdio.h>
void prime_Factorization(long long n) {
    while (n %2==0) {
        printf("%d ",2);
        n =n/2;
    }
    for (long long i =3;i*i<= n; i =i+2) {
        while (n %i ==0) {
            printf("%lld ",i);
            n =n/i;
        }
    }
    if (n >2) {
        printf("%lld",n);
    }
}
int main() {
    long long N;
    printf("Enter a number: ");
    scanf("%lld", &N);
    printf("Prime factors: ");
    prime_Factorization(N);
    printf("\n");
    return 0;
}
